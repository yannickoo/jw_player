<div class="jwplayer-video">
    <video id="{{ html_id }}" width="{{ width }}" height="{{ height }}" controls="controls" preload="none"{% if poster %} poster="{{ poster }}"{%  endif %}>
        <source src="{{ file_url }}"{% if file_mime %} type="{{ file_mime }}" {% endif %}/>
    </video>
</div>
{% if  jw_player_inline_js_code  %}
    <script type="text/javascript">
        {% autoescape false %}
        {% if license_key %}
        jwplayer.key = {{ license_key }}
        {% endif %}
        jwplayer('{{ html_id }}').setup( {{ jw_player_inline_js_code }})
        {% endautoescape %};
    </script>
{% endif %}
